[user]
	email = ivenvd@gmail.com
	name = iven
[alias]
    ci = commit -a
    df = diff --word-diff=color
    dc = diff --word-diff=color --cached
    st = status -sb
    br = branch
    co = checkout
    lg = log --color -p
    who = shortlog -s --
    ps = push origin --all
[giggle]
	main-window-maximized = true
	main-window-geometry = 0x0+0+0
	history-view-vpane-position = 84
	file-view-vpane-position = 367
	main-window-view = FileView
[color]
	ui = auto
[diff]
    wordregex = ",|.|=|'|\"|(|)|[^[:space:]()='\".,]+"
[core]
	excludesfile = /home/iven/.global_ignore
